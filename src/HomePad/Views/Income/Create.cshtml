@model HomePad.Models.IncomeVM
@{
    ViewData["Index"] = "Income";
 }




@* <script type="text/javascript">
    $(document).ready(function() {
        alert("document loaded");
    });
</script> *@

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="col-md-2">
        @Html.DropDownListFor(inc => inc.AccountHeadId,
        new SelectList(ViewBag.accountHeads, "Id", "Name"),
        "Select AccountHead",
        new { @class = "form-control" })

    </div>

    <div class="form-group">
        @Html.TextBoxFor(inc=> inc.TransactionDate)
    </div>

    <div class="col-md-2">
        <label asp-for="Title" class="control-label"></label>
        <input asp-for="Title" class="form-control" />
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>

    <div class="col-md-2">
        <label asp-for="Amount" class="control-label"></label>
        <input asp-for="Amount" class="form-control" />
        <span asp-validation-for="Amount" class="text-danger"></span>
    </div>

    <div class="form-group">
        @Html.TextBoxFor(inc=> inc.TransactionDate)
    </div>

    <div class="col-md-2">
        <label asp-for="LastUpdatedBy" class="control-label"></label>
        <input asp-for="LastUpdatedBy" class="form-control" />
        <span asp-validation-for="LastUpdatedBy" class="text-danger"></span>
    </div>

    <div class="col-md-2">
        <label asp-for="Attachment" class="control-label"></label>
        <input asp-for="Attachment" class="form-control" />
        <span asp-validation-for="Attachment" class="text-danger"></span>
    </div>

    <div class="col-md-2">
        <label asp-for="Note" class="control-label"></label>
        <input asp-for="Note" class="form-control" />
        <span asp-validation-for="Note" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>

</form>

